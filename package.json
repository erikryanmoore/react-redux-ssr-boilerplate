{
  "name": "react-redux-ssr-boilerplate",
  "engines": {
    "node": "10.15.1",
    "yarn": "1.13.0"
  },
  "version": "0.1.0",
  "description": "React-Redux Server Side Rendered Boilerplate",
  "main": "index.js",
  "repository": "git@github.com:erikryanmoore/react-redux-ssr-boilerplate.git",
  "author": "erikryanmoore <erikmoore02@gmail.com>",
  "license": "GPL-3.0",
  "scripts": {
    "dev": "npm-run-all -p dev:client:watch dev:server:watch dev:watch",
    "dev:client": "webpack --config webpack.client.dev.babel.js",
    "dev:client:watch": "yarn dev:client --watch",
    "dev:server": "webpack --config webpack.server.dev.babel.js",
    "dev:server:watch": "yarn dev:server --watch",
    "dev:watch": "nodemon --watch build --exec \"node build/server/server.js\"",
    "dev-setup": "npm-run-all -p dev:client dev:server",
    "lint": "$(npm bin)/eslint src",
    "prod": "npm run prod:server && npm run prod:client",
    "prod:client": "webpack --config webpack.client.prod.babel.js",
    "prod:server": "webpack --config webpack.server.prod.babel.js",
    "start": "node dist/server/server.js",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@babel/runtime": "^7.3.1",
    "body-parser": "^1.18.3",
    "connected-react-router": "^6.2.2",
    "express": "^4.16.4",
    "history": "^4.7.2",
    "prop-types": "^15.6.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-helmet": "^5.2.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^1.6.1",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-mocha": "^5.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.3",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.3",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
